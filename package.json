{
  "name": "cartracker",
  "version": "0.0.3",
  "author": "tentone",
  "description": "Car GPS tracking application using the ionic framework.",
  "homepage": "https://github.com/tentone",
  "scripts": {
    "start": "ng serve",
    "build": "ionic build",
    "run-pwa": "npx cap serve",
    "run-android": "ionic build && npx cap sync android && cd android && gradlew.bat assembleDebug && gradlew.bat installDebug && cd ..",
    "open-android": "ionic build && npx cap sync android && npx cap open android",
    "open-ios": "ionic build && npx cap sync ios && npx cap open ios"
  },
